{
  "compilerOptions": {
    "jsx": "react",
    "module": "ES6",
    "noImplicitAny": true,
    "outDir": "./dist",
    "preserveConstEnums": true,
    "removeComments": true,
    "sourceMap": true,
    "target": "es5",
    "allowSyntheticDefaultImports": true
  },
  "include": ["./src/"],
  "exclude": ["node_modules"]
}
